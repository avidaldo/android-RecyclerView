<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.example.android_recyclerview.MainFragment"
        android:label="Main Fragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_to_Ej01aFragment"
            app:destination="@id/ej01aFragment" />
        <action
            android:id="@+id/action_to_Ej01bFragment"
            app:destination="@id/ej01bFragment" />
        <action
            android:id="@+id/action_to_ej02Fragment"
            app:destination="@id/ej02Fragment" />
        <action
            android:id="@+id/action_to_ej03Fragment"
            app:destination="@id/ej03Fragment" />
        <action
            android:id="@+id/action_to_ej04Fragment"
            app:destination="@id/ej04Fragment" />
        <action
            android:id="@+id/action_to_ej05Fragment"
            app:destination="@id/ej05Fragment" />
        <action
            android:id="@+id/action_MainFragment_to_ej06Fragment"
            app:destination="@id/ej06Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ej01aFragment"
        android:name="com.example.android_recyclerview.Ej01aFragment"
        android:label="Ejemplo 01a"
        tools:layout="@layout/fragment_ej01a"/>
    <fragment
        android:id="@+id/ej01bFragment"
        android:name="com.example.android_recyclerview.Ej01bFragment"
        android:label="Ejemplo 01b"
        tools:layout="@layout/fragment_ej01b"/>
    <fragment
        android:id="@+id/ej02Fragment"
        android:name="com.example.android_recyclerview.Ej02Fragment"
        android:label="Ejemplo 02"

    <fragment
        android:id="@+id/ej03Fragment"
        android:name="com.example.android_recyclerview.Ej03Fragment"
        android:label="Ej03Fragment" />
    <fragment
        android:id="@+id/ej04Fragment"
        android:name="com.example.android_recyclerview.Ej04Fragment"
        android:label="Ej04Fragment" />
    <fragment
        android:id="@+id/ej05Fragment"
        android:name="com.example.android_recyclerview.ej05.Ej05Fragment"
        android:label="Ej05Fragment" />
    <fragment
        android:id="@+id/ej06Fragment"
        android:name="com.example.android_recyclerview.ej06.Ej06Fragment"
        android:label="fragment_item_list"
        tools:layout="@layout/fragment_item_list" />
